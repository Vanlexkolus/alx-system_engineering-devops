#!/usr/bin/env bash

# Loop through files in the current directory (excluding hidden files)
for file in *; do
    # Check if the file is not a hidden file
    if [[ ! $file == .* ]]; then
        # Use cut command to extract the part of the name after the first dash
        name=$(echo "$file" | cut -d'-' -f2-)
        # Print the extracted name
        echo "$name"
    fi
done
